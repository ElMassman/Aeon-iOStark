<window id="0">
<defaultcontrol always="true">300</defaultcontrol>
<allowoverlay>yes</allowoverlay>

<controls>
  
	<control type="wraplist" id="300">
		<description>Fake Home Menu</description>
		<posx>0</posx>
		<posy>0</posy>
		<width>1</width>
		<height>1</height>
		<visible>-</visible>
    		<onup>300</onup>
    		<ondown>300</ondown>
		<onleft>300</onleft>
		<onright>300</onright>
   		<focusposition>0</focusposition>
		<scrolltime>200</scrolltime>
		<itemlayout height="1">
		</itemlayout>
		<focusedlayout height="1">
		</focusedlayout>
      		<content>
		</content>
	</control>

	<control type="multiimage">
		<width>1280</width>
		<height>720</height>
		<aspectratio>stretch</aspectratio>
		<imagepath>../../../../../../../../Library/Desktop Pictures/Nature</imagepath>
		<timeperimage>8000</timeperimage>
		<fadetime>1000</fadetime>
		<randomize>true</randomize>
		<loop>yes</loop>
	</control>

	<control type="group">

		<Description>RSS Feed Bar</Description>
	
		<posy>663</posy>

			<control type="image">
				<posx>54</posx>
				<posy>0</posy>
				<width>1253</width>
				<height>57</height>
				<texture flipy="true">common/rssbar_mid.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			
			<control type="rss" id="1">
			    <posx>110</posx>
			    <posy>35</posy>
			    <width>1280</width>
			    <font>Font_RSS</font>
			    <urlset>1</urlset>
			    <include>Colour_RSS</include>
			</control>

		</control>

	<include>Clock</include>
  
</controls>
  
</window>